// Test: List Operations
// This test checks if list operations work properly

bandhig("Testing list operations:")

// Creating lists
liis empty_list = []
liis numbers = [10, 20, 30, 40, 50]
liis mixed = [1, "two", been, [1, 2], {name: "something"}]

bandhig("  Empty list: " + empty_list)
bandhig("  Numbers list: " + numbers)
bandhig("  Mixed list: " + mixed)

// Accessing elements
bandhig("  First element of numbers: " + numbers[0])
bandhig("  Third element of numbers: " + numbers[2])
bandhig("  Last element of numbers: " + numbers[4])

// Modifying elements
numbers[1] = 25
bandhig("  After changing second element: " + numbers)

// List length
bandhig("  List length: " + numbers.dherer())

// Push operation
numbers.kudar(60)
bandhig("  After push: " + numbers)
numbers.kudar(70)
bandhig("  After another push: " + numbers)

// Pop operation
door popped = numbers.kasaar()
bandhig("  Popped value: " + popped)
bandhig("  List after pop: " + numbers)

// Nested list access
liis nested = [[1, 2], [3, 4]]
bandhig("  Nested list: " + nested)
bandhig("  Accessing nested element [1][0]: " + nested[1][0])

// List iteration
bandhig("  List iteration using for loop:")
kuceli (i 0 ilaa numbers.dherer() - 1) {
    bandhig("    Element " + i + ": " + numbers[i])
}

// List concatenation
liis list1 = [1, 2, 3]
liis list2 = [4, 5, 6]
liis combined = list1.kudar(list2)
bandhig("  List1: " + list1)
bandhig("  List2: " + list2)
bandhig("  Combined list: " + combined)

// Check if element exists in list
tiro search_value = 30
bool contains = numbers.leeyahay(search_value)
bandhig("  Does numbers contain " + search_value + "? " + contains)

search_value = 100
contains = numbers.leeyahay(search_value)
bandhig("  Does numbers contain " + search_value + "? " + contains)
